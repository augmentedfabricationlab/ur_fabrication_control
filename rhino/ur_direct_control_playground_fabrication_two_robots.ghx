                    <item name="CodeInput" type_name="gh_string" type_code="10">from scriptcontext import sticky as st

import sys, os
am_hackathon_path = os.path.abspath(os.path.join(ghdoc.Path,"../../src"))
if am_hackathon_path not in sys.path:
    sys.path.append(am_hackathon_path)

from compas.geometry import Frame
from compas.geometry import Translation
from compas.geometry import Transformation
from compas.geometry import Vector
from compas_ghpython import update_component
from mobile_additive_manufacturing.control import URScript_Extrusion
from mobile_additive_manufacturing.control import set_extruder_data
from ur_fabrication_control.direct_control import URScript
from ur_fabrication_control.direct_control.mixins.airpick_mixins import AirpickMixins
from ur_fabrication_control.direct_control.fabrication_process import Fabrication

ur_ip_1 = st["ur_ip_1"]
ur_port_1 = st["ur_port_1"]
ur_ip_2 = st["ur_ip_2"]
ur_port_2 = st["ur_port_2"]
server_ip = st["server_address"]
server_port = st["server_port"]
ext_ip = st["ext_ip"]
ext_port = st["ext_port"]

safe_level = 0.200
element_num = 1
robot_2_cf = Frame(robot_2_cp.Origin, robot_2_cp.XAxis, robot_2_cp.YAxis)
T_rcf2 = Transformation.from_frame_to_frame(robot_2_cf, Frame.worldXY())

pick_frame = Frame(assembly_pickup.Origin, assembly_pickup.YAxis, assembly_pickup.XAxis)

class URScript_Airpick(URScript, AirpickMixins):
    pass

def create_tasks_from_am_model(am_model):
    for key, layer in am_model.layers():
        if key == 0 or key in arch_layer:
            toggle_extruder = True
        elif key == am_model.number_of_layers()-1:
            toggle_extruder = False
        else:
            toggle_extruder = None
        if key in arch_layer:
            stop_script = get_stop_ur_script(am_safety_plane)
            exit_message = "Element{}_placed".format(arch_layer.index(key))
            assembly_script = get_pick_and_place_ur_script(assembly.element(arch_layer.index(key)), exit_message)
            fabrication.add_task(assembly_script, exit_message)
        exit_message = "Layer{}_completed".format(key)
        layer_urscript = get_layer_ur_script(layer, toggle_extruder, exit_message)
        fabrication.add_task(layer_urscript, exit_message)

def get_frames_in_RCS(frames):
    return [robot_1.to_local_coordinates(frame) for frame in frames]

def get_layer_ur_script(layer, toggle_extruder, exit_message):
    ur_script = URScript_Extrusion(ur_ip=ur_ip_1, ur_port=ur_port_1, ext_ip=ext_ip, ext_port=ext_port)
    tool_angle_axis = list(robot_1.attached_tool.frame.point) + list(robot_1.attached_tool.frame.axis_angle_vector)
    
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    RCS_frames = get_frames_in_RCS([node.frame for key, node in layer.nodes()])
    first = False
    for key, node in layer.nodes():
        if not first:
            ur_script.move_linear(robot_1.to_local_coordinates(node.frame), velocity=ur_speed, radius=node.radius)
            first = True
            if (toggle_extruder == True) and (toggle_extruder != None) and include_extruder:
                toggle_extrusion(ur_script, int(toggle_extruder))
        else:
            ur_script.move_process(robot_1.to_local_coordinates(node.frame), ur_speed, node.radius)
    else:
        if (toggle_extruder == False) and (toggle_extruder != None) and include_extruder:
            toggle_extrusion(ur_script, int(toggle_extruder))
    # Signify ready
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.socket_send_line_string(exit_message, socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")
    #Finalize script
    ur_script.end()
    ur_script.generate()
    return ur_script

def get_pick_and_place_ur_script(element, exit_message):
    tool_angle_axis = list(robot_2.attached_tool.frame.point) + list(robot_2.attached_tool.frame.axis_angle_vector)
    ur_script = URScript_Airpick(ur_ip=ur_ip_2, ur_port=ur_port_2)
    ur_script.start()
    ur_script.add_airpick_commands()
    ur_script.set_tcp(tool_angle_axis)
    vec = [sum(x) for x in zip(assembly_pickup.XAxis*arch_radii[int(exit_message[7])], assembly_pickup.ZAxis*arch_radii[element_num])]
    T1 = Translation.from_vector(vec)
    T2 = Translation.from_vector([0,0,safe_level])
    T3 = Translation.from_vector([0,0,safe_level*2])
    pick_frame_RCS = pick_frame.transformed(T_rcf2)
    pick_frame_elem = pick_frame_RCS.transformed(T1)
    pick_safe_level = pick_frame_elem.transformed(T2)
    tool_frame_RCS = element.tool_frame.transformed(T_rcf2)
    target_safe_level = tool_frame_RCS.transformed(T3)
    
    ur_script.move_linear(pick_safe_level, ur_speed*5, radius=0.1)
    ur_script.move_linear(pick_frame_elem, ur_speed*2)
    ur_script.airpick_on(max_vac=75, min_vac=25)
    ur_script.move_linear(pick_safe_level, ur_speed*5, radius=0.1)
    ur_script.move_linear(target_safe_level, ur_speed*5, radius=0.1)
    ur_script.move_linear(tool_frame_RCS, ur_speed*2)
    ur_script.airpick_off(pressure=80, timeout=5)
    ur_script.move_linear(target_safe_level, ur_speed*5, radius=0.1)
    ur_script.move_linear(pick_safe_level, ur_speed*5, radius=0.1)
    # Signify ready
    ur_script.socket_open(server_ip, server_port, "Feedbackserver")
    ur_script.socket_send_line_string(exit_message, socket_name="Feedbackserver")
    ur_script.socket_close(name="Feedbackserver")
    ur_script.end()
    ur_script.generate()
    return ur_script

def get_stop_ur_script(safety_plane):
    safety_frame_WCS = Frame(safety_plane.Origin, safety_plane.XAxis, safety_plane.YAxis)
    safety_frame_RCS = robot_1.to_local_coordinates(safety_frame_WCS)
    
    ur_script = URScript_Extrusion(ur_ip=ur_ip_1, ur_port=ur_port_1, ext_ip=ext_ip, ext_port=ext_port)
    tool_angle_axis = list(robot_1.attached_tool.frame.point) + list(robot_1.attached_tool.frame.axis_angle_vector)
    
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    if include_extruder:
        toggle_extrusion(ur_script, 0)
    ur_script.move_linear(safety_frame_RCS, velocity=ur_speed, radius=0)
    ur_script.end()
    ur_script.generate()
    return ur_script

