import math
from compas.geometry import Frame, Transformation, Rotation
from compas_fab.robots import Configuration
from ur_fabrication_control.kinematics.ur_kinematics import inverse_kinematics
from ur_fabrication_control.kinematics.ur_params import ur_params
from compas_ghpython import draw_frame

plane_WCS = planes_WCS[plane_nr]

tcp_WCS = Frame(plane_WCS.Origin, plane_WCS.XAxis, plane_WCS.YAxis)

# transform frame to robot coordinate system
tcp_RCS = robot.to_local_coordinates(tcp_WCS)
tool0_RCS = robot.from_tcf_to_t0cf([tcp_RCS])[0]

print tcp_RCS
print tool0_RCS
plane_tool0_RCS = draw_frame(tool0_RCS)
plane_tcp_RCS = draw_frame(tcp_RCS)
# calculate solutions to frame
ur_params = ur_params[robot.name] # get the parameters according to the robot model
solutions = inverse_kinematics(tool0_RCS, ur_params)

if not len(solutions):
    joint_values = [0,0,0,0,0,0]
    configuration = Configuration.from_revolute_values(joint_values)
else:
    joint_values = solutions[idx]
    #print joint_values
    
    ### ***** HACK ***** ##### TODO: CHECK!!!
    for i, v in enumerate(joint_values):
        if v &gt; math.pi:
            v -= 2*math.pi
        if v &lt; -math.pi:
            v += 2*math.pi
        joint_values[i] = v
    ### ***** HACK ***** ##### TODO: CHECK!!!
    
    configuration = [Configuration.from_revolute_values(joint_values)]

print(configuration)
