                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math


if len(poses) &gt; 2:
    
    O = poses[0]
    P1 = poses[1]
    P2 = poses[2]
    
    if input_type == 0:
        # origin, pt on positive x-axis, pt in xy plane
        origin = rg.Point3d(O[0], O[1], O[2])
        pt_on_xaxis = rg.Point3d(P1[0], P1[1], P1[2])
        pt_in_xyplane = rg.Point3d(P2[0], P2[1], P2[2])
        
        xaxis = pt_on_xaxis - origin
        xaxis.Unitize()
        
        v = pt_in_xyplane - origin
        zaxis = rg.Vector3d.CrossProduct(v, xaxis)
        zaxis.Unitize()
        
        # zaxis must look up!
        if math.acos(rg.Vector3d.ZAxis * zaxis) &gt; math.acos(rg.Vector3d.ZAxis * -zaxis):
            zaxis = -zaxis
        
        yaxis = rg.Vector3d.CrossProduct(zaxis, xaxis)
    
    elif input_type == 1:
        # origin, pt on positive y-axis, pt in xy plane
        origin = rg.Point3d(O[0], O[1], O[2])
        pt_on_yaxis = rg.Point3d(P1[0], P1[1], P1[2])
        pt_in_xyplane = rg.Point3d(P2[0], P2[1], P2[2])
        
        yaxis = pt_on_yaxis - origin
        yaxis.Unitize()
        
        v = pt_in_xyplane - origin
        zaxis = rg.Vector3d.CrossProduct(v, yaxis)
        zaxis.Unitize()
        
        # zaxis must look up!
        if math.acos(rg.Vector3d.ZAxis * zaxis) &gt; math.acos(rg.Vector3d.ZAxis * -zaxis):
            zaxis = -zaxis
        
        xaxis = rg.Vector3d.CrossProduct(yaxis, zaxis)
        
        
    print "Copy the following into base component:"
    print " "
    print "origin = [%.2f, %.2f, %.2f]" % tuple(list(origin))
    print "xaxis = [%.5f, %.5f, %.5f]" % tuple(list(xaxis))
    print "yaxis = [%.5f, %.5f, %.5f]" % tuple(list(yaxis))
    

#print rg.Vector3d.CrossProduct(rg.Vector3d.YAxis, rg.Vector3d.ZAxis)
#print rg.Vector3d.CrossProduct(rg.Vector3d.ZAxis, rg.Vector3d.XAxis)

    base_plane = rg.Plane(origin, xaxis, xaxis)</item>
