                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
from compas.geometry import Frame
from compas.geometry import Transformation, Rotation
from compas_ghpython import draw_frame
from compas_fab.robots import robot
from ur_fabrication_control import direct_control 
import math

from scriptcontext import sticky as st
from ur_fabrication_control.direct_control import URScript

robot = st["robot"]

# retrieve the current tool frame in axis angle format
if robot.attached_tool:
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
else:
    tool0_frame = Frame([0, 0, 0], [1, 0, 0], [0, 1, 0])
    tool_angle_axis = list(tool0_frame.point) + list(tool0_frame.axis_angle_vector)

frames_RCS = []
for plane_WCS in planes_WCS:
    frame_WCS = Frame(plane_WCS.Origin, plane_WCS.XAxis, plane_WCS.YAxis)
    frame_RCS = robot.to_local_coordinates(frame_WCS)
    frames_RCS.append(frame_RCS)

urscript = URScript()
urscript.start()
urscript.moves_process(frames_RCS, velocity, radius)
urscript.end()

urscript = urscript.generate()

print(urscript)
