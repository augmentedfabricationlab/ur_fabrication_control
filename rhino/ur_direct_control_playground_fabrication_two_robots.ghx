from compas_ghpython import update_component
from am_hackathon.control import URScript_Extrusion

ur_ip = st["ur_ip"]
ur_port = st["ur_port"]
ext_ip = st["ext_ip"]
ext_port = st["ext_port"]

layers = [layer for key, layer in am_model.layers()]
ur_script = URScript_Extrusion(ur_ip=ur_ip, ur_port=ur_port, ext_ip=ext_ip, ext_port=ext_port)

def get_frames_in_RCS(frames):
    return [robot.to_local_coordinates(frame) for frame in frames]

def get_ur_script(layer):
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
    ur_script.start()
    ur_script.set_tcp(tool_angle_axis)
    if include_extruder:
        ur_script.open_socket_extruder()
        ur_script.set_extruder_data(speed=ext_speed, acceleration=ext_accel)
        #ur_script.toggle_fan(state=1)
        ur_script.toggle_air(state=1)
    RCS_frames = get_frames_in_RCS([node.frame for key, node in layer.nodes()])
    T = Translation.from_vector(Vector(0,0,safe_level))
    safeframe_start = RCS_frames[0].transformed(T)
    safeframe_end = RCS_frames[-1].transformed(T)
    ur_script.move_linear(safeframe_start, velocity=ur_speed*25, radius=0)
    first = False
    for key, node in layer.nodes():
        if not first:
            ur_script.move_linear(robot.to_local_coordinates(node.frame), velocity=ur_speed*10, radius=node.radius)
            first = True
            if include_extruder:
                ur_script.toggle_drv8825(state=1)
                ur_script.add_line('\tsleep(1)')
                ur_script.toggle_extruder(state=1)
                ur_script.add_line('\tsleep(1)')
        else:
            ur_script.move_process(robot.to_local_coordinates(node.frame), ur_speed, node.radius)
    if include_extruder:
        ur_script.toggle_extruder(state=0)
        ur_script.add_line('\tsleep(1)')
        ur_script.toggle_drv8825(state=0)
    ur_script.move_linear(safeframe_end, velocity=ur_speed*25, radius=0)
    if include_extruder:
        ur_script.toggle_air(state=0)
        #ur_script.toggle_fan(state=0)
        ur_script.add_line('\tsleep(1)')
        ur_script.close_socket_extruder()
    ur_script.end()
    ur_script.generate()
    return ur_script


