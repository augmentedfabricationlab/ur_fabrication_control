from ur_fabrication_control.direct_control.common import get_current_pose_cartesian
from compas.geometry import Frame
from compas_ghpython import draw_frame

robot = st["robot_1"]

server_address = st["server_address"]
server_port = st["server_port"]
ur_port = st["ur_port_1"]
ur_ip = st["ur_ip_1"]

# retrieve the current tool frame in axis angle format
if robot.attached_tool:
    tool_angle_axis = list(robot.attached_tool.frame.point) + list(robot.attached_tool.frame.axis_angle_vector)
else:
    tool0_frame = Frame([0, 0, 0], [1, 0, 0], [0, 1, 0])
    tool_angle_axis = list(tool0_frame.point) + list(tool0_frame.axis_angle_vector)

print(tool_angle_axis)

guid = str(ghenv.Component.InstanceGuid)
pose_cartesian_key = "pose_cartesian" + guid


if get:
    st[pose_cartesian_key] = get_current_pose_cartesian(tool_angle_axis, server_address, server_port, ur_ip, ur_port, send=True)

if pose_cartesian_key not in st:
    pose_cartesian = []
else:
    pose_cartesian = st[pose_cartesian_key]

print(pose_cartesian)

# get frame and plane of the current pose cartesian in world coordinates for visualization
if pose_cartesian:
    frame_RCS = Frame.from_axis_angle_vector(axis_angle_vector=pose_cartesian[3:], point=pose_cartesian[:3])
    frame_WCS = robot.to_world_coordinates(frame_RCS)
    plane_WCS  = draw_frame(frame_WCS)
    print frame_RCS
    print frame_WCS</item>
